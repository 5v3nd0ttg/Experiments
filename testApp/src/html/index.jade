            script(type='text/javascript').
                var feedcontainer = document.getElementById("feed");
                var feedurl = "https://github.com/NathanielInman.atom";
                var feedlimit = 10;
                var curColor = false;
                var resizeFeed = function () {
                    var w = window,
                        d = document,
                        e = d.documentElement,
                        g = d.getElementsByTagName('body')[0],
                        x = w.innerWidth || e.clientWidth || g.clientWidth;
                    if (x >= 1470) {
                        document.getElementById("leftFeed").style.visibility = "visible";
                    } else {document.getElementById("leftFeed").style.visibility = "hidden";}
                };
                var changeCurListening = function () {
                    if (curColor == false) {
                        document.getElementById("curListening").style.color = "#888";
                        curColor = true;
                    } else {
                        document.getElementById("curListening").style.color = "#9C6";
                        curColor = false;
                    } //end if
                };
                var changeCurListeningTimer = setInterval(function () {changeCurListening();}, 500);
                var rssoutput = "";
                function rssfeedsetup() {
                    var feedpointer = new google.feeds.Feed(feedurl)
                    feedpointer.setNumEntries(feedlimit)
                    feedpointer.load(displayfeed)
                } //end rssfeedsetup()
                function displayfeed(result) {
                    if (!result.error) {
                        var thefeeds = result.feed.entries
                        for (var i = 0, bq = 0; i < thefeeds.length && !(typeof thefeeds[i] == "undefined"); i++) {
                            rssoutput += "<a href='" + thefeeds[i].link + "'>" + thefeeds[i].title.replace('lI', 'l I').replace('NathanielInman/', '') + "</a><br/>";
                            bq = thefeeds[i].content.indexOf('<blockquote>');
                            if (bq >= 0) {
                                for (var j = bq + 12, k = j; i < thefeeds[i].content.length; j++) {
                                    if (thefeeds[i].content.substring(j, j + 13) == '</blockquote>') {
                                        rssoutput += '<b>' + thefeeds[i].content.substring(k, j) + '</b><br/>';
                                        break;
                                    } //end if
                                } //end for
                            }
                            rssoutput += "<span style='color:#555;font-style:italic'>" + thefeeds[i].publishedDate.replace(' -0700', '') + "</span><br/>";
                            if (i < thefeeds.length - 1)rssoutput += '<br/>';
                        } //end for
                        feedcontainer.innerHTML = rssoutput
                    } else {
                        if (console)console.log("Error fetching feeds!");
                    } //end if
                } //end displayfeed()
                window.onload = function () {
                    rssfeedsetup(); //set up the data for the feed
                    resizeFeed(); //initialize the feed, should it be visible?
                }
